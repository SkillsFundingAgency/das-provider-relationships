@{
    var configuration = (GoogleAnalyticsConfiguration)ViewBag.GoogleAnalyticsConfiguration;
}

@if (configuration.ContainerId != null)
{
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=@configuration.ContainerId"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
}