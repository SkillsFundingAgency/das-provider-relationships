<Project Sdk="Microsoft.NET.Sdk;Microsoft.NET.Sdk.Publish">

  <PropertyGroup>
    <IsGenerateRunCommandFileDisabled>true</IsGenerateRunCommandFileDisabled>
    <IsWebJobProject>true</IsWebJobProject>
    <OutputType>Exe</OutputType>
    <TargetFramework>net462</TargetFramework>
    <WebJobName>ProviderRelationships-MessageHandlers</WebJobName>
    <WebJobType>Continuous</WebJobType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.WebJobs" Version="2.2.0" />
    <PackageReference Include="Microsoft.VisualStudio.SlowCheetah" Version="3.1.66" />
    <PackageReference Include="NLog.Schema" Version="4.5.10" />
    <PackageReference Include="SFA.DAS.NLog.Logger" Version="1.1.5" />
    <PackageReference Include="SFA.DAS.NLog.Targets.Redis" Version="1.1.5" />
    <PackageReference Include="SFA.DAS.NServiceBus" Version="3.0.1" />
    <PackageReference Include="SFA.DAS.NServiceBus.SqlServer" Version="3.0.1" />
    <PackageReference Include="SFA.DAS.UnitOfWork.EntityFramework" Version="2.0.1" />
    <PackageReference Include="SFA.DAS.UnitOfWork.NServiceBus" Version="2.0.1" />
    <PackageReference Include="StructureMap" Version="4.7.0" />
    <PackageReference Include="WindowsAzure.Storage" Version="9.3.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SFA.DAS.ProviderRelationships.Messages\SFA.DAS.ProviderRelationships.Messages.csproj" />
    <ProjectReference Include="..\SFA.DAS.ProviderRelationships\SFA.DAS.ProviderRelationships.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System" />
    <Reference Include="System.Configuration" />
  </ItemGroup>

  <ItemGroup>
    <None Include="App.config">
      <TransformOnBuild>true</TransformOnBuild>
    </None>
    <None Include="App.*.config">
      <DependentUpon>App.config</DependentUpon>
      <IsTransformFile>True</IsTransformFile>
    </None>
  </ItemGroup>

</Project>
